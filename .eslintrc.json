{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "camelcase":"off",
        "max-len":"off",
        "object-curly-newline":"off",
        "radix":"off",
    }
}
